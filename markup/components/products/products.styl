.products
  background-color: #DDE2EB
  &__no-active
    visibility hidden
  &__inner
    padding-top 80px
    padding-bottom 130px
    xs-block(@block{
      padding-top 60px
      padding-bottom 60px
    })
  &__title
    margin-bottom 80px
    padding-left 20px
    font-size 32px
    line-height 40px
    color #37474f
    font-weight 400
    text-transform uppercase
    xxs-block(@block{
      margin-bottom 40px
    })
  &__list
    row-flex()
    justify-content space-between
    flex-wrap wrap
    from-to(481px, 935px, @block{
      justify-content center
      display block
    })
  &__item
    col()
    size(4)
    size-xs(12)
    display flex
    flex-direction column
    align-items center
    justify-content stretch
    padding 50px 40px 40px
    background-color #fff
    border-radius 10px
    box-shadow: 0 10px 30px 0 rgba(0,0,0,0.16)
    text-align center
    transition: all 0.2s cubic-bezier(0.445, 0.05, 0.55, 0.95)
    lg-block(@block{
      padding 40px
    })
    from-to(481px, 935px, @block{
      width 80%
      margin-left 0
      margin 0 auto
    })
    &:not(:last-child)
      from-to(481px, 935px, @block{
        margin-bottom 30px

      })
    xs-block(@block{
      margin-bottom 20px
      padding 40px 20px 20px
    })
    &:hover,
    &:focus
      transform: translateY(-10px) ;
      box-shadow: 0 10px 30px 0 rgba(0,0,0,0.16)
      xs-block(@block{
        transform none
        box-shadow none
      })

    p
      margin-bottom 25px
      font-size 14px
      line-height 18px
      font-weight 300
      color #8898B3
    a:not(.products__btn)
      position relative
      display block
      color #5C8AE6
      font-size 12px
      line-height 22px
      font-weight 400
      text-transform uppercase
      &:hover
        text-decoration underline
  &__item-title
    margin-bottom 20px
    font-size 24px
    line-height 30px
    font-weight 400
    color #37474f
    lg-block(@block{
      margin-bottom 30px
    })
  &__icon-wrapper
    width 100px
    height 100px
    margin-bottom 20px
    &--design
      background-image url('data:image/svg+xml;base64,')
      background-repeat no-repeat
    &--view
      background-image url('data:image/svg+xml;base64,')
      background-repeat no-repeat
    &--plan
      background-image url('data:image/svg+xml;base64,')
      background-repeat no-repeat
  &__btn
    margin-top auto
    width 170px
    margin-bottom 20px
    padding 13px 0
    font-size 14px
    line-height 18px
    font-weight 400
    color #fff
    border-radius 25px
.side-line
  border-left 2px solid #ff7043
